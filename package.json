  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-standard": "^6.1.0",
    "babel-eslint": "10.0.1",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
